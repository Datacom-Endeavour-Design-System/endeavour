{
  "name": "@datacom/endeavour-storybook",
  "version": "0.0.1",
  "private": true,
  "description": "Endeavour storybook",
  "scripts": {
    "clean": "rimraf dist storybook-static",
    "dev": "npm run start",
    "lint": "eslint src",
    "start": "cross-env STORYBOOK_STORIES=unstable storybook dev -p 6006",
    "build": "storybook build",
    "build:unstable": "cross-env STORYBOOK_STORIES=unstable storybook build",
    "test": "cypress run --component --config video=false",
    "test:interactive": "cypress open --component"
  },
  "dependencies": {
    "@datacom/endeavour": "*",
    "@datacom/endeavour-react": "*",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@storybook/addon-actions": "7.4.5",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-interactions": "7.4.5",
    "@storybook/addon-links": "7.4.5",
    "@storybook/addons": "7.4.5",
    "@storybook/react-vite": "7.4.5",
    "@storybook/testing-library": "0.2.1",
    "@storybook/theming": "7.4.5",
    "@types/node": "^18.11.7",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "babel-loader": "^8.2.5",
    "replace-in-file": "^6.3.5",
    "storybook": "7.4.5",
    "vite": "^3.2.0",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "author": "endeavour@datacom.co.nz",
  "license": "ISC",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/storybook-static"
        ]
      }
    }
  },
  "peerDependencies": {
    "@types/react": "^18.2.24",
    "@types/react-dom": "18.2"
  }
}
