{
  "name": "@datacom/endeavour-react",
  "version": "0.2.0",
  "description": "React specific wrapper for Datacom design systems web components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "lint": "eslint src",
    "clean": "rimraf dist src/components/stencil-generated",
    "rollup": "rollup -c"
  },
  "dependencies": {
    "@datacom/endeavour": "*"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "np": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "author": "endeavour@datacom.co.nz",
  "license": "ISC",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
