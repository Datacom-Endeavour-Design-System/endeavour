{
  "name": "@datacom/endeavour-react",
  "version": "0.0.1",
  "description": "React specific wrapper for Datacom design systems web components",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "lint": "eslint src",
    "clean": "rimraf dist src/components/stencil-generated",
    "rollup": "rollup -c"
  },
  "dependencies": {
    "@datacom/endeavour": "*"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "author": "endeavour@datacom.co.nz",
  "license": "ISC",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
